
public class Jefe extends Empleado {
	static int plus = 250;
	String nombreDepart;

	Jefe(String nombre, int edad, int porcentajeIncremento, String nombreDepart) {
		this.nombre = nombre;
		this.edad = edad;
		this.porcentajeIncremento = porcentajeIncremento;
		this.nombreDepart = nombreDepart;
	}

	public void setNombreDepart(String nombreDepart) {
		this.nombreDepart = nombreDepart;
	}

	Jefe() {
		nombreDepart = "";

	}

	public String toString() {
		return "Empleado:" + nombreEmpresa + " nombre " + edad + " años " + sueldoFinal + "€.";
	}

}
