import java.util.Arrays;

public class Pila {
	static Integer[] pila = new Integer[500];
	static int index = 0;

	public static int pop() {
		int valor = pila[index];
		pila[index] = null;
		return valor;
	}

	public void add(int entero) {
		pila[index]=entero;
	}

	public Pila copy() {
		return this;
	}

	public void borrar() {
		for (int i = 0; i < pila.length; i++) {
			pila[i] = null;
		}
	}

	public boolean vacia() {
		if (pila[0] == null) {
			return true;
		} else {
			return false;
		}
	}
	public static Pila invertir(){
		
	}
}
