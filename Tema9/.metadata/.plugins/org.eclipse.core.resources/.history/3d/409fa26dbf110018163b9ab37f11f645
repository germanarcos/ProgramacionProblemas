import leer.Leer;

public class main {
	public static void main(String[] args) {
		int botones;
		int dom;
		switch (Leer.pedirEntero("1 muestra 0 sale")) {
		case 1:
			do {
				botones = Leer.pedirEntero("Cuantos botones quieres");
				dom=(int) Math.pow((int) Math.sqrt(botones), 2);
			} while (dom!= botones);
			Campo.Campo(botones);
			break;
		default:
			System.out.println("bieen");
			break;
		}
	}
}
